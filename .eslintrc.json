{
    "root": true,
    "ignorePatterns":["dist/*"],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any":0,
      "@typescript-eslint/no-unused-vars":1,
      "no-empty-function": "off",
      "@typescript-eslint/no-empty-function": ["warn"]
    }
}