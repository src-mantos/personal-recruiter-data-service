{
    "name": "data-service",
    "version": "1.0.0",
    "description": "pulling data from recruiting api's",
    "main": "main.ts",
    "author": "mantos",
    "license": "MIT",
    "scripts": {
        "build": "npx tsc --build",
        "exec": "npx ts-node",
        "docs": " npx typedoc --entryPointStrategy expand ./src",
        "serve": "yarn build && node -r source-map-support/register ./dist/src/server.js",
        "debug": "yarn build && node --trace-warnings -r source-map-support/register ./dist/src/debug.js",
        "debugRunner": "yarn build && node -r source-map-support/register ./dist/src/debugRunner.js",
        "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register",
        "check": "jest --config ./test/jest.config.ts /test/driver.test.ts",
        "testy": "mocha -r ts-node/register --full-trace --inline-diffs './test/dao/DaoIntegration.test.ts'",
        "lint": "eslint .",
        "lint-fix": "eslint . --fix --quiet"
    },
    "devDependencies": {
        "@types/chai": "^4.3.3",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/express-ws": "^3.0.1",
        "@types/mocha": "^10.0.0",
        "@types/node": "^16.11.13",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.8.1",
        "@typescript-eslint/parser": "^5.8.1",
        "chai": "^4.3.6",
        "eslint": "^8.6.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.2.5",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.0.1",
        "jsdoc": "^3.6.7",
        "mocha": "^10.1.0",
        "source-map-support": "^0.5.21",
        "standard": "^17.0.0",
        "ts-node": "^10.4.0",
        "typedoc": "^0.22.0",
        "typescript": "^4.5.4"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "express": "^4.18.1",
        "express-jsdoc-swagger": "^1.8.0",
        "express-validator": "^6.14.2",
        "express-ws": "^5.0.2",
        "mongoose": "^6.4.0",
        "playwright": "^1.17.1",
        "reflect-metadata": "^0.1.13",
        "swagger-ui-express": "^4.5.0",
        "tsyringe": "^4.6.0",
        "uuid": "^8.3.2"
    }
}
