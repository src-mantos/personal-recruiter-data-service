{
    "printWidth": 120,
    "useTabs": false,
    "tabWidth": 4,
    "semi": true,
    "bracketSpacing": true,
    "singleQuote": true,
    "quoteProps": "consistent",
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "bracketSameLine": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "embeddedLanguageFormatting": "off"
}
